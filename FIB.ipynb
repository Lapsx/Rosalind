    "def recurrence(lista):\n",
    "    f = open(lista,'r')\n",
    "    info = f.read()\n",
    "    f.close()\n",
    "    \n",
    "    info = info.split(' ')\n",
    "    n,k = map(int,(info[0], info[1]))\n",
    "    \n",
    "    a,b = 1,1\n",
    "    for i in range(n-1):\n",
    "        a,b = b, b+(a*k)\n",
    "    return a"
   ]
